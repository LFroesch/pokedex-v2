{
  "name": "pokedex-v2",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm install --prefix frontend && npm run build --prefix frontend && cd backend && go build -o main .",
    "start": "cd backend && ./main -web",
    "dev:frontend": "npm run dev --prefix frontend",
    "dev:backend": "cd backend && go run . -web",
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "install:frontend": "npm install --prefix frontend",
    "install:backend": "cd backend && go mod tidy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LFroesch/pokedex-v2.git"
  },
  "keywords": [],
